import {useState} from 'react';
import logo from './logo.svg';
import './App.css';

function App() 
{
  const [data,setData]=useState(0);
 function updateData()
 {
   setData(data+1);
   alert("done");
 }
  return (
    <div className="App">
      <h1>{data}</h1>
      <button type='button' onClick={updateData}>click me for alert</button>
    </div>
  );
}

export default App;
